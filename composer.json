{
  "name": "yourdutchmedia/laravel-livewire-datatables",
  "description": "A dynamic data table component for Laravel Livewire",
  "keywords": [
    "laravel",
    "livewire"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/contracts": "^8.44",
    "livewire/livewire": "^2.4",
    "spatie/laravel-package-tools": "^1.9"
  },
  "require-dev": {
    "orchestra/testbench": "^6.18",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Ydm\\Datatables\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ydm\\Datatables\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ydm\\Datatables\\DatatableServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
